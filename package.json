{
  "name": "@vulppi/kit",
  "version": "0.0.1",
  "description": "Backend kit make by Vulppi",
  "license": "MIT",
  "author": {
    "name": "Renato Rodrigues",
    "email": "renato@vulppi.dev"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/vulppi-dev/kit.git"
  },
  "keywords": [
    "vulppi",
    "backend",
    "kit",
    "express",
    "typescript",
    "esbuild",
    "dotenv",
    "dotenv-expand",
    "rest",
    "api",
    "rest-api",
    "api-rest",
    "http",
    "http-status-codes",
    "http-status",
    "status-codes",
    "framework",
    "scalable",
    "worker",
    "workers",
    "worker-threads",
    "worker-thread",
    "thread",
    "threads"
  ],
  "main": "dist/lib/index.mjs",
  "module": "dist/lib/index.mjs",
  "types": "types/index.d.ts",
  "bin": {
    ".": "./bin/index.mjs",
    "vulppi": "./bin/index.mjs"
  },
  "exports": {
    ".": {
      "import": "./dist/lib/index.mjs",
      "require": "./dist/lib/index.cjs"
    },
    "./types/*": {
      "types": "./types/*"
    }
  },
  "scripts": {
    "dev": "nodemon --watch src -e js,cjs,mjs,ts,json scripts/build.mjs",
    "build": "node scripts/build.mjs",
    "test": "run-s build test:start",
    "test:start": "uvu -r dotenv-expand/config -r esbuild-register"
  },
  "dependencies": {
    "@vulppi/toolbelt": "^0.6.3",
    "chalk": "^5.3.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "esbuild": "^0.18.11",
    "express": "^4.18.2",
    "glob": "^10.3.3",
    "http-status-codes": "^2.2.0",
    "inquirer": "^9.2.7",
    "lodash": "^4.17.21",
    "range-parser": "^1.2.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/inquirer": "^9.0.3",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.1",
    "@types/range-parser": "^1.2.4",
    "@types/yargs": "^17.0.24",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.1.6"
  }
}
